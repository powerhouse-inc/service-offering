{
  "name": "@powerhousedao/service-offering",
  "description": "service offering document models",
  "version": "0.0.2",
  "license": "AGPL-3.0-only",
  "type": "module",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./document-models": {
      "types": "./dist/document-models/index.d.ts",
      "default": "./dist/document-models/index.js"
    },
    "./editors": {
      "types": "./dist/editors/index.d.ts",
      "default": "./dist/editors/index.js"
    },
    "./document-models/*": {
      "types": "./dist/document-models/*/index.d.ts",
      "default": "./dist/document-models/*/index.js"
    },
    "./editors/*": {
      "types": "./dist/editors/*/index.d.ts",
      "default": "./dist/editors/*/index.js"
    },
    "./subgraphs": {
      "types": "./dist/subgraphs/index.d.ts",
      "default": "./dist/subgraphs/index.js"
    },
    "./processors": {
      "types": "./dist/processors/index.d.ts",
      "default": "./dist/processors/index.js"
    },
    "./manifest": {
      "default": "./dist/powerhouse.manifest.json"
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "build": "npm run tsc && npm run tailwind",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint --config eslint.config.js --cache --cache-strategy content",
    "lint:fix": "npm run lint -- --fix",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "tailwind": "pnpm exec tailwindcss -i ./style.css -o ./dist/style.css",
    "prepublishOnly": "npm run build",
    "check-circular-imports": "npx dpdm -T ./index.ts",
    "generate": "ph-cli generate",
    "connect": "ph-cli connect",
    "reactor": "ph-cli reactor",
    "service": "ph-cli service",
    "vetra": "ph-cli vetra",
    "service-startup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-startup.sh",
    "service-unstartup": "bash ./node_modules/@powerhousedao/ph-cli/dist/scripts/service-unstartup.sh",
    "migrate": "ph-cli migrate"
  },
  "dependencies": {
    "@powerhousedao/builder-tools": "latest",
    "@powerhousedao/common": "latest",
    "@powerhousedao/design-system": "latest",
    "@powerhousedao/document-engineering": "1.40.0",
    "@powerhousedao/vetra": "latest",
    "@uiw/react-md-editor": "^4.0.11",
    "document-model": "latest",
    "error": "^10.4.0",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "lucide-react": "^0.563.0",
    "rehype-slug": "^6.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@electric-sql/pglite": "^0.2.12",
    "@eslint/js": "^9.38.0",
    "@powerhousedao/analytics-engine-core": "^0.5.0",
    "@powerhousedao/codegen": "latest",
    "@powerhousedao/config": "latest",
    "@powerhousedao/connect": "latest",
    "@powerhousedao/ph-cli": "latest",
    "@powerhousedao/reactor-api": "latest",
    "@powerhousedao/reactor-browser": "latest",
    "@powerhousedao/reactor-local": "latest",
    "@powerhousedao/switchboard": "latest",
    "@tailwindcss/cli": "^4.1.4",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "document-drive": "latest",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.4.0",
    "package-manager-detector": "^0.2.8",
    "pm2": "^5.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.1.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^6.2.3",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
